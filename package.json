{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@arco-design/mobile-react": "^2.19.1",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "add": "^2.0.6",
    "ahooks": "^3.7.0",
    "react": "17.x",
    "react-dom": "17.x",
    "react-transition-group": "^4.4.5",
    "umi": "^3.5.32",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.4",
    "@umijs/plugin-antd-mobile": "^1.2.0",
    "@umijs/test": "^3.5.32",
    "amfe-flexible": "^2.2.1",
    "lint-staged": "^10.0.7",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
